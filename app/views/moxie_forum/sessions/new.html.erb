<% form_for(@account, :url => session_path)  do |f| %>
<fieldset>
<legend>Log In</legend>
<ul>
  <li><%= f.label :uname, "User Name:" %></li>
  <li><%= f.text_field :uname %></li>
  <li><%= f.label :password, "Password: " %></li>
  <li><%= f.password_field :password %></li>
  <li class="remember-me"><%= f.check_box :remember_me %><%= f.label :remember_me, "Remember Me"%></li>
  <li class="submit"><%= f.submit 'Log In' %></li>
</ul>
</fieldset>
<% end %>
